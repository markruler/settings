{
  "default": true,
  "extends": null,
  "first-line-h1": true,
  "first-heading-level": { "level": 1 },
  "heading-style": { "style": "consistent" },
  "ul-style": { "style": "consistent" },
  "list-indent": true,
  "start-left": true,
  "no-trailing-spaces": {
    "br_spaces": 2,
    "list_item_empty_lines": false,
    "strict": false
  },
  "hard-tabs": { "code_blocks": true },
  "no-reversed-links": true,
  "no-multiple-blanks": { "maximum": 1 },
  "line-length": false,
  "commands-show-output": true,
  "no-missing-space-atx": true,
  "no-missing-space-closed-atx": true,
  "no-missing-space-setext": true,
  "no-missing-space-headers": true,
  "blanks-around-headers": {
    "lines_above": 1,
    "lines_below": 1
  },
  "no-duplicate-header": true,
  "no-duplicate-header-siblings": { "siblings_only": false },
  "single-title": {
    "level": 1,
    "front_matter_title": "^\\s*title\\s*[:=]"
  },
  "no-trailing-punctuation": {
    "punctuation": ".,;:!。，；：！"
  },
  "no-empty-links": true,
  "blank-lines-around-blockquote": true,
  "ordered-list-marker-style": { "style": "one_or_ordered" },
  "list-marker-space": {
    "ul_single": 1,
    "ol_single": 1,
    "ul_multi": 1,
    "ol_multi": 1
  },
  "blanks-around-fences": true,
  "inline-html": { "allowed_elements": [] },
  "no-bare-urls": true,
  "horizontal-rule-style": { "style": "consistent" },
  "no-space-in-links": true,
  "no-space-in-code": true,
  "no-space-in-inline-code": true,
  "no-space-in-blockquote": true,
  "first-line-h1-title": {
    "level": 1,
    "front_matter_title": "^\\s*title\\s*[:=]"
  },
  "no-empty-sections": true,
  "proper-names-in-code": {
    "names": [],
    "code_blocks": true
  },
  "no-duplicate-definitions": true,
  "code-block-style": { "style": "consistent" },
  "no-empty-blockquote": true
}
